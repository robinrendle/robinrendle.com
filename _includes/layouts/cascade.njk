<div class="grid">
  <div class="window grid-column-12">
    <div class="window-title">
      <h2>What?</h2>
    </div>
    <div class="window-content">
      <h1 class="heading window-content-salutations" id="heading">{{ title }}</h1>
    </div>
  </div>
</div>



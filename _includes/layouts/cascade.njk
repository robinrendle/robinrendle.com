---
layout: layouts/base.njk
templateClass: no-footer
---

<style>
  .cursor {
    height: 0;
    width: 0;
    display: none;
  }

  .window-content p {
    margin-top: 10px;
  }

  .cursor-animate {
    width: 25px;
    opacity: 0;
    animation: cursor 4s linear infinite;
  }

  .cursor-wrapper-clouds svg:nth-child(1n) {
    fill: var(--color-text);
    animation: clouds 2s linear infinite;
    animation-direction: alternate;
    margin-bottom: 5px;
  }

  @keyframes clouds {
    0% {
      transform: skew(15deg, 15deg);
    } 100% {
      transform: skew(0);
    }
  }           

  @keyframes cursor {
    0% {
      opacity: 0;
    } 100% {
      opacity: 1;
    }
  }

  .cursor-wrapper-mail svg {
    fill: var(--color-text);
    animation: 2s mail linear infinite;
    animation-direction: alternate;
    margin-bottom: 5px;
  }

  @keyframes mail {
    0% {
      transform: scale(0.4);
    } 100% {
      transform: scale(0.9, 0.8);
    }
  }

  .cursor-wrapper-cursors svg {
    fill: var(--color-text);
    opacity: 0.8;
    margin-bottom: 10px;
    animation: wiggle 3s ease-in infinite;
    animation-direction: alternate;
  }

  .window-cascade-title .window-content {
    font-size: 22px;
    line-height: 1.3;
    min-height: 300px;

    .inputs {
      margin-top: 70px;
    }
  }

  h2 {
    margin-top: 10px;
    margin-bottom: 10px;
    {# font-size: 35px; #}
    line-height: 1.2;
  }

  @keyframes wiggle {
    0% {
      transform: rotate(220deg);
    } 50% {
      transform: rotate(225deg);
    } 60% {
      transform: rotate(210deg);
    } 70% {
      transform: rotate(215deg);
    } 80% {
      transform: rotate(220deg);
    } 100% {
      transform: rotate(220deg);
    }
  }
</style>


---
layout: layouts/base.njk
templateClass: no-footer
---

<style>
  :root {
    --font: "Arial";
  }

  .cursor {
    height: 0;
    width: 0;
    display: none;
  }
  
  h1 {
    font-family: var(--font);
    height: 150px;
    display: grid;
    align-items: center;
    font-size: clamp(3rem, 18vw, 8rem)
  }

  h2 {
    margin: 0;
    line-height: 1.3;
  }

  .window-content p {
    margin-top: 10px;
  }

  .inputs {
    margin-top: 20px;
  }

  .cursor-wrapper {
    display: flex;
    gap: -4px;
  }

  .cursor-animate {
    width: 30px;
    opacity: 0;
    animation: cursor 4s linear infinite;
  }

  .cursor-animate:nth-child(1n) {
    animation-delay: 0s;
  }

  .cursor-animate:nth-child(2n) {
    animation-delay: 0.5s;
  }

  .cursor-animate:nth-child(3n) {
    animation-delay: 1s;
  }

  .cursor-animate:nth-child(4n) {
    animation-delay: 1.5s;
  }

  .cursor-animate:nth-child(5n) {
    animation-delay: 2s;
  }

  .cursor-animate:nth-child(6n) {
    animation-delay: 2.5s;
  }

  .cursor-animate:nth-child(7n) {
    animation-delay: 3s;
  }

  .cursor-animate:nth-child(8n) {
    animation-delay: 3.5s;
  }

  .cursor-animate:nth-child(9n) {
    animation-delay: 4s;
  }

  .icon-hourglass {
    width: 50px;
    height: 50px;
    display: grid;
    place-items: center;
    animation: 5s turntime linear infinite;
    margin-bottom: 5px;
  }

  @keyframes turntime {
    0% {
      transform: rotate(0deg);
    } 50% {
      transform: rotate(180deg);
    } 60% {
      transform: rotate(180deg);
    }
    100% {
      transform: rotate(180deg);
    }
  }                

  @keyframes cursor {
    0% {
      opacity: 0;
      {# transform: translate(0, -5px); #}
    } 100% {
      opacity: 1;
      {# transform: translate(-1px); #}
    }
  }
</style>


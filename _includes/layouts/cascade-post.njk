---
layout: layouts/base.njk
templateClass: no-footer
---

<div class="grid">
  <article class="window grid-column-12">
    <div class="window-title">
      <h1 id="heading">{{ title }}</h1>  
      <time class="window-title-meta" datetime="{{ page.date | htmlDateString }}">
        {{ page.date | fullDate }}
      </time>
    </div>
    <div class="window-content">
      <div class="article-wrapper">
        {{ content | safe }}
      </div>
    </div>
  </article>
</div>

<div class="grid">
  <div class="window grid-column-8">
    <div class="window-title">
      <h2>The Cascade</h2>
    </div>
    <div class="window-content window-content-wrapper">
      <h2 class="newsletter-title">The Cascade is a newsletter about the past, present, and future of CSS by your friendly neighborhood Robin Rendle.</h2>

      <form
        action="https://buttondown.email/api/emails/embed-subscribe/cascade"
        method="post"
        target="popupwindow"
        onsubmit="window.open('https://buttondown.email/cascade', 'popupwindow')"
        class="embeddable-buttondown-form"
      >
        <div class="inputs">
          <input type="email" name="email" id="bd-email" placeholder="Email address" />
          <input type="submit" value="Subscribe" />
        </div>
      </form>
    </div>
  </div>

<div class="grid">
  {% for post in collections.cascade | reverse %}
    <a class="window window-link window-cascade-post grid-column-4 {{loop.index}}" href="{{ post.url }}">
      <div class="window-content window-content-wrapper">
        {% set total = loop.length - loop.index %}
        <div class="notes-list-item-index">{{total}}</div>
        <div>
          <h3 class="notes-list-title">{{ post.data.title }}</h3>
          <p class="notes-list-extract">{{ post.data.extract }}</p>
        </div>
      </div>
    </a>
  {% endfor %}
</div>
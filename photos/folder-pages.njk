---
pagination:
  data: photoNav
  size: 1
  alias: folder
permalink: "/photos/{{ folder.slug }}/"
layout: layouts/base.njk
template: photo
eleventyComputed:
  title: "{{ folder.name }}"
---

{% for photo in folder.photos %}
  <a class="gallery-item" href="{{ photo.url }}">
    <img class="gallery-img" src="{{ photo.src }}" alt="{{ photo.filename }}">
  </a>
{% endfor %}
